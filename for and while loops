// print the numbers 1 to 5

#include <stdio.h>

int i;
int main() {
    for (i=1; i<=5; i++){
        printf("%d\n", i);
    }
}

// print the numbers 5 to 1

#include <stdio.h>

int i;
int main() {
    for (i=5; i>=1; i--){
        printf("%d\n", i);
    }
}

// print the sum of numbers 1 to 5

#include <stdio.h>

int i, sum=0;
int main() {
    for (i=1; i<=5; i++){
        sum= sum+i;
    }
    printf("%d\n", sum);
}

// print the sum of numbers 6 to 1

#include <stdio.h>

int i, sum=0;
int main() {
    for (i=6; i>=1; i--){
        sum= sum+i;
    }
    printf("%d\n", sum);
}

// print the odd numbers from 10 to 20

#include <stdio.h>

int i;
int main() {
    for (i=10; i<=20; i++){
        if(i%2==1){
            printf("%d\n", i);
        }
    }
}

// print if the number is odd and the sum of digits is 7

#include <stdio.h>

int i;
int main() {
    for (i=10; i<=100; i++){
        if((i%2==1) && (i%10 + i/10 == 7)){
            printf("%d\n", i);
        }
    }
}

// print the sum of 2 digit numbers whose ones digit is 5

#include <stdio.h>

int i, sum=0;
int main() {
    for (i=10; i<=100; i++){
        if(i%10==5){
            printf("%d\n", i);
            sum= sum+i;
        }
    }
    printf("%d", sum);
}

// print the sum of two digit odd number whose tens digit is 7

#include <stdio.h>

int i, sum=0;
int main() {
    for (i=10; i<=100; i++){
        if((i/10==7)&& (i%2==1)){
            printf("%d\n", i);
            sum= sum+i;
        }
    }
    printf("%d", sum);
}

// print the number of digits fo the number

#include <stdio.h>

int n, count=0;
int main() {
    printf("Enter the number: ");
    scanf("%d", &n);
    while(n>0){
        n=n/10;
        count= count+1;
    }
    printf("%d", count);
}

// print the sum of the digits of the given number

#include <stdio.h>

int n, x, sum=0;
int main() {
    printf("Enter the number: ");
    scanf("%d", &n);
    while(n>0){
        x=n%10;
        sum= sum+x;
        n=n/10;
    }
    printf("%d", sum);
}

// print the revese of the number

#include <stdio.h>

int n, rev=0, digit=1;
int main() {
    printf("Enter the number: ");
    scanf("%d", &n);
    while(n>0){
        digit=n%10;
        rev=rev*10 +digit;
        n=n/10;
    }
    printf("%d", rev);
}

// interchange only the first and last digit

#include <stdio.h>

int n, temp, last, div=1, middle, first, number;
int main() {
    printf("Enter the number: ");
    scanf("%d", &n);
    temp=n;
    last= n%10;
    
    while(temp>=10){
        temp=temp/10;
        div=div*10;
    }
    first= temp;
    middle= (n%div)/10;
    number= (last*div)+ (middle*10)+ first;
    printf("%d", number);
}

// print the number as it is if the last digit is even else subtract 1 from the digit and return the number

#include <stdio.h>

int n, temp, last, div=1, number, result;
int main() {
    printf("Enter the number: ");
    scanf("%d", &n);
    temp=n;
    
    while(temp>=10){
        temp=temp/10;
        div=div*10;
    }
    last= temp;
    number= n % div;
    
    if (temp%2==1){
        temp=temp-1;
        result= (temp*div)+ number;
        printf("%d", result);
    }
    else{
        printf("%d", n);
    }
}

// prime or not

#include <stdio.h>

int n,i;
int main() {
    printf("Enter the number: ");
    scanf("%d", &n);
    
    if(n<=1){
        printf("it is not a prime number");
        return 0;
    }
    for(i=2; i<= n/2; i++){
        if(n%i ==0){
            printf("It is not a prime number");
        }
    }
    
    printf("It is a prime number");
}

// prime and sum of digits is 14 or not, 4 conditions

#include <stdio.h>

int main() {
    int n, temp, sum = 0, i;
    int isPrime = 1;

    printf("Enter the number: ");
    scanf("%d", &n);

    if (n <= 1) {
        isPrime = 0;
    } else {
        for (i = 2; i <= n / 2; i++) {
            if (n % i == 0) {
                isPrime = 0;
                break;
            }
        }
    }

    temp = n;
    while (temp > 0) {
        sum = sum + (temp % 10);
        temp = temp / 10;
    }

    if (isPrime && sum == 14)
        printf("Prime & sum of digits is 14");
    else if (!isPrime && sum == 14)
        printf("Not Prime but sum of digits is 14");
    else if (isPrime && sum != 14)
        printf("Prime & sum of digits is not 14");
    else
        printf("Not Prime & sum of digits is not 14");

    return 0;
}

// get a number and check the last two digit of the numbe is prime or not

#include <stdio.h>

int n,i, x;
int main() {
    printf("Enter the number: ");
    scanf("%d", &n);
    
    x= n%100;
    
    if(x<=1){
        printf("it is not a prime number");
        return 0;
    }
    for(i=2; i<= x/2; i++){
        if(x%i ==0){
            printf("It is not a prime number");
            return 0;
        }
    }
    
    printf("It is a prime number");
}

// get a 4 digit number and check whether the  middle two digit number is prime or not

#include <stdio.h>

int n,i, x,y;
int main() {
    printf("Enter 4 digit number: ");
    scanf("%d", &n);
    
    y= n/10;
    x= y%100;
    
    if(x<=1){
        printf("it is not a prime number");
        return 0;
    }
    for(i=2; i<= x/2; i++){
        if(x%i ==0){
            printf("It is not a prime number");
            return 0;
        }
    }
    
    printf("It is a prime number");
}

// print the number of single digit prime numbers

#include <stdio.h>

int j,i, count=0;
int isPrime;

int main() {
    
    
    for (i = 2; i <= 10; i++) {
        isPrime =1;
        
        for(j=2; j<i; j++){
            if(i%j==0){
                isPrime=0;
                break;
            }
        }
        
        if(isPrime==1){
            count=count+1;
        }
    }
    
    printf("%d", count);
  
}

// get a number and print the number of odd digits in the number

#include <stdio.h>

int n, count=0, x;

int main() {
    printf("Enter the number: ");
    scanf("%d", &n);
    
    while (n>0){
        x= n%10;
        if (x%2==1){
            count = count+1;
        }
        
        n= n/10;
    }
    printf("%d", count);
}

// get a number and print the number of two digit odd number in the number

#include <stdio.h>

int n, count=0, x;

int main() {
    printf("Enter the number: ");
    scanf("%d", &n);
    
    while (n>0){
        x= n%100;
        if (x%2==1){
            count = count+1;
        }
        
        n= n/10;
    }
    printf("%d", count);
}

// get a number and print the number of single digit perfect square

#include <stdio.h>

int n, count=0, x, i;

int main() {
    printf("Enter the number: ");
    scanf("%d", &n);
    
    while (n>0){
        x= n%10;
        for(i=1; i<10; i++){
            if (i*i==x){
            count = count+1;
            }
        }
        
        n= n/10;
    }
    printf("%d", count);
}

// get a number and print the number of 2 digit perfect square number in the number

#include <stdio.h>

int n, count=0, x, i;

int main() {
    printf("Enter the number: ");
    scanf("%d", &n);
    
    while (n>10){
        x= n%100;
        for(i=1; i<100; i++){
            if (i*i==x){
            count = count+1;
            }
        }
        
        n= n/10;
    }
    printf("%d", count);
}


// print the largest 4 digit number which is div by both 7 and 9

#include <stdio.h>

int x, i;

int main() {
    
    for (i=1000; i< 10000; i++){
       if (i%7==0 && i%9==0){
           x=i;
       }
    }
    printf("%d\n", x);
}

// print the total count of numbers less than 10000 whose sum of digits is 14

#include <stdio.h>

int main() {
    int i, temp, sum, count = 0;

    for (i = 1; i < 100000; i++) {
        temp = i;
        sum = 0;

        while (temp > 0) {
            sum = sum + (temp % 10);
            temp = temp / 10;
        }

        if (sum == 14) {
            count++;
        }
    }

    printf("%d\n", count);
    return 0;
}

// LCM of two numbers

#include <stdio.h>

int main() {
    int a, b, max;

    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);

    max = (a > b) ? a : b;

    while (1) {
        if (max % a == 0 && max % b == 0) {
            printf("LCM = %d", max);
            break;
        }
        max++;
    }

    return 0;
}


// print the LCM of 3 numbers

#include <stdio.h>

int main() {
    int a, b, c, max;

    printf("Enter three numbers: ");
    scanf("%d %d %d", &a, &b, &c);
    
    if (a>b && a>c){
        max=a;
    }else if (b>a && b>c){
        max=b;
    }else {
        max=c;
    }

    while (1) {
        if (max % a == 0 && max % b == 0 && max % c==0) {
            printf("LCM = %d", max);
            break;
        }
        max++;
    }
}

// print the HCF of 2 numbers

#include <stdio.h>

int main() {
    int a, b, i, hcf;

    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);

    for (i = 1; i <= a && i <= b; i++) {
        if (a % i == 0 && b % i == 0) {
            hcf = i;
        }
    }

    printf("HCF = %d", hcf);
}
